{
  "name": "@acme/database-with-sqlite",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .turbo node_modules",
    "with-env": "dotenv -e ../../.env",
    "db-migration": "pnpm with-env drizzle-kit generate",
