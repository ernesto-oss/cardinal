    "db-drop": "pnpm with-env drizzle-kit drop --out=drizzle"
  },
  "dependencies": {
    "@acme/config": "file:../../config/with-next-auth-database/",
    "better-sqlite3": "^8.4.0",
    "drizzle-orm": "^0.26.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.4",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.18.1",
    "typescript": "^5.0.4"
  }
}
